{
  "name": "centrifuge",
  "productName": "Centrifuge",
  "version": "1.0.0",
  "description": "Desktop tool for inspecting narrative patterns within the Talk of the Town simulation-based story generator",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "clean": "rimraf ./.webpack ./out yarn-error.log ./dist",
    "lint": "eslint --ext ./src"
  },
  "keywords": [],
  "author": {
    "name": "Shi Johnson-Bey",
    "email": "shijbey@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "talktown_inspector"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@projectstorm/react-diagrams": "^6.5.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.168",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-loader": "^8.2.2",
    "bootstrap": "^4.6.0",
    "chart.js": "^3.2.1",
    "classnames": "^2.3.1",
    "closest": "^0.0.1",
    "css-loader": "^4.2.1",
    "dagre": "^0.8.5",
    "electron": "12.0.2",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "fs-extra": "^9.1.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "jquery": "1.9.1",
    "lodash": "4.*",
    "node-loader": "^1.0.1",
    "pathfinding": "^0.4.18",
    "paths-js": "^0.4.11",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-chartjs-2": "^3.0.3",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-is": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.12",
    "sass-loader": "10.1.1",
    "source-map-loader": "^2.0.1",
    "style-loader": "^1.2.1",
    "styled-components": "^5.2.3",
    "ts-loader": "^8.1.0",
    "typescript": "^4.0.2",
    "uuid": "^8.3.2",
    "webpack": "^5.31.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
